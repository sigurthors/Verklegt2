@model List<BookCave.Models.ViewModels.CartViewModel>

<h1>YOUR BASKETTTTTTTTT</h1>
<table>
    <tr>
        <th>Title</th>
        <th>Price</th>
    </tr>
    @foreach(var Item in @Model)
    {
        <tr>
            <td>@Item.Title</td>
            <td>@Item.Price</td>
        </tr>
        
    }
</table>