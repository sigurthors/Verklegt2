@model List<BookCave.Models.ViewModels.BookListViewModel>

<div>
    <h1>10 Vinsælustu bækur</h1>
    @foreach(var Books in Model)
    {
        <h2>@Books.Title</h2>
        <p>Útgáfuár: @Books.ReleaseYear</p>
        <p>Einkunn: @Books.Rating</p>
        <p>Höfundur: @Books.Author</p>
        <br>
    }
</div>